package com.techlabs.man;

public class Man {
	
	public void read() {
		System.out.println("Man is reading");
	}
	
	public void wish() {
		System.out.println("Man is wishing");
	}

	public void play() {
		System.out.println("Man is playing");
	}
	
	public static void addTheGround(Man x) {
		System.out.println("At the Ground");
		x.play();
	}
}
